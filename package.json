{
  "name": "chimemate",
  "displayName": "ChimeMate",
  "author": "Grace Chen Abudi",
  "description": "A GitHub productivity VS Code extension that automatically tracks and logs developers coding activity every 30 minutes, creating a time-based record with each commit.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "license": "MIT",
  "keywords": [
    "vscode",
    "vscode-extension",
    "productivity",
    "github",
    "git-commit",
    "tracking",
    "tracking-logs",
    "coding-activity",
    "developer-productivity",
    "time-tracking",
    "git-tracking",
    "commit-log",
    "open-source",
    "code-log",
    "dev-tools",
    "chimeMate",
    "automated-commits",
    "github-contribution",
    "developer-tools",
    "coding-dashboard"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.startTracking",
        "title": "Start Tracking"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/simple-oauth2": "^5.0.7",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.16.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.2",
    "simple-oauth2": "^5.1.0"
  }
}
